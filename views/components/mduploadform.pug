#markdown-upload-form(uk-modal='')
  form.uk-modal-dialog.uk-modal-body.uk-margin-auto-vertical(action='/upload/markdown', method='POST', enctype='multipart/form-data')
    button.uk-modal-close-default(type='button' uk-close='' style='left:unset;')
    .uk-margin
      .uk-inline.uk-width-1-1
        a.uk-form-icon.uk-form-icon-flip(href="" uk-icon="icon: pencil")
        input#signin-id-input.uk-input.uk-width-1-1(placeholder="제목을 입력하세요" autocomplete="off")
    .uk-margin
      input#markdown.uk-input(type='file', name='markdown', accept='image/*')
    #image-upload-btn.uk-margin
      button.uk-button.uk-button-primary.uk-width.uk-width-1-1 업로드